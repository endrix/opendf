
actor Expand (choice, errorf) In, Next ==> Out :

	A:
		action In: [v] ==> [w] 
		var
			choices = v["choice"] + [choice],
			error = v["error"] + errorf(choices, v["Y"]),
			w = map {"choices" -> choices, "error" -> error}
		end
		
	B: 
		action Next: [_] ==> end
		
	schedule fsm s0:
		s0 (A) --> s1;
		s1 (B) --> s0;
	end
end

