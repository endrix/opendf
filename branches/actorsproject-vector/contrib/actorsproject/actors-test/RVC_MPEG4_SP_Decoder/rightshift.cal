// ECARVON modified: comprehension->loop
actor rightshift () int In ==> int Out :

  action In:[ x ] repeat 64 ==> Out: [ x ] repeat 64
  var
    int i
  do
    // [ rshift(x[i], 13) : for int i in Integers(0, 63) ]
    i:=0;
    while (i<64) do
      x[i]:=rshift(x[i], 13);
      i:=i+1;
    end
  end

 end