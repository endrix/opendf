actor variableChecks() inPort ==> outPort :

  myAction: action inPort:[ x ] ==> outPort:[ x ] end
  
  action inPort:[x] ==> end
  
  schedule fsm undefinedInitialState :

    goodState( myAction ) --> noWayOut;
    noWayOut ( myAction ) --> deadEndState;
    badState( undefinedAction ) --> goodState;
    anotherBadState( myAction.undefined ) --> goodState;
    noWayIn  ( myAction ) --> goodState;
  end
  
end