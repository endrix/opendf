
<AttributeFolding>
    
    <ConnectionAttributeDefault attrKind="Value">
        <FoldingFunction value="lambda (L) : L end"/>
    </ConnectionAttributeDefault>
        
    <ConnectionAttribute attrName="bufferSize" attrKind="Value">
        <!-- <DefaultValue value="1"/> -->
        <FoldingFunction value="lambda (L) : accumulate(lambda (x, y) : if x > y then x else y end end, 0, L) end"/>
    </ConnectionAttribute>
    
    <ConnectionAttribute attrName="initialTokens" attrKind="Value">
        <!-- <DefaultValue value="1"/> -->
        <FoldingFunction value="lambda (L) : accumulate(lambda (x, y) : y + x end, [], L) end"/>
        <!--<FoldingFunction value="lambda (L) : L end"/> -->
    </ConnectionAttribute>
    
    <ConnectionAttribute attrName="queuetap" attrKind="Value">
        <FoldingFunction value="lambda (L) : accumulate(lambda (x, y) : x or y end, false, L) end"/>
    </ConnectionAttribute>
    
    <ConnectionAttribute attrName="debugtap" attrKind="Value">
        <FoldingFunction value="lambda (L) : accumulate(lambda (x, y) : x or y end, false, L) end"/>
    </ConnectionAttribute>

    <InstanceAttribute attrName="clockDomain" attrKind="Value">
        <FoldingFunction value="lambda (L) : L[0] end"/>
        <DefaultValue value='"CLK"'/>
    </InstanceAttribute>
    
    <InstanceAttribute attrName="sdcConstraint" attrKind="Value">
        <FoldingFunction value="lambda (L) : L end"/>
    </InstanceAttribute>
    
    <InstanceAttribute attrName="vhdlEntity" attrKind="Value">
        <FoldingFunction value="lambda (L) : L[0] end"/>
    </InstanceAttribute>
    
    <InstanceAttribute attrName="partname" attrKind="Value">
      <FoldingFunction value="lambda (L) : L[0] end"/>
    </InstanceAttribute>
    
    <InstanceAttribute attrName="xmlElement" attrKind="Value">
      <FoldingFunction value="lambda (L) : L[0] end"/>
    </InstanceAttribute>

</AttributeFolding>
